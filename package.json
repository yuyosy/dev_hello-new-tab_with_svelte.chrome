        "@tsconfig/svelte": "^3.0.0",
        "@types/chrome": "^0.0.193",
        "svelte": "^3.49.0",
        "svelte-check": "^2.8.0",
        "svelte-preprocess": "^4.10.7",
        "typescript": "^4.7.4",
        "vite": "^2.9.14",
        "web-ext": "^7.1.1"
    }
}
